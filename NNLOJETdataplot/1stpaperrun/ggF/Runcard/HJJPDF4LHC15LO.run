PROCESS  HJJ
  decay_type = 3
  collider = pp
  sqrts = 13000
  jet = antikt[0.4]
  jet_exclusive = .false.
  jet_recomb = V4
END_PROCESS

RUN  HLO  ! <--- this is the JOB name
  PDF = PDF4LHC15_nnlo_30[0]
  tcut = 1d-7
  iseed   = 11
  imethod = 2
  iplot   = 0
  lips_reduce      = .true.
  reset_vegas_grid = .true.
  angular_average  = .false.
  print_max_weight = .false.
  cache_kinematics = .true.
  pole_check       = .false.
  multi_channel    = .false.
  !multi_channel    = 2
  !phase_space      = qt

  warmup     = 1000000[10]
  !production = 1000000[10]
END_RUN

PARAMETERS
  MASS[H]  = 125
  WIDTH[H] = 0.004088 !Higgs values from https://twiki.cern.ch/twiki/bin/view/LHCPhysics/CERNYellowReportPageBR#TotalWidthAnchor
  MASS[Z]  = 91.1876
  WIDTH[Z] = 2.4952
  MASS[W]  = 80.379
  WIDTH[W] = 2.085 !Vector boson values from http://pdg.lbl.gov/2019/download/db2018.pdf
  MASS[t]  = 173.055
  WIDTH[t] = 1.41
  MASS[b]  = 0
  MASS[c]  = 0
  MASS[tau]  = 0
  SCHEME[WIDTH_V] = CMS
END_PARAMETERS


SELECTORS

        select njets        min = 2
  	select jets_pt      min = 30
  	select jets_abs_y   max = 4.4
  	select abs_yh       max = 2.4

END_SELECTORS

